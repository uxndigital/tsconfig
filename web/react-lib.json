{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "../base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.react-lib.tsbuildinfo",
    "allowJs": true,
    "composite": true,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "sourceMap": true
  }
}
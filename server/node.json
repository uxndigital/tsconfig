{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node Server",
  "extends": "../base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "composite": true,
    "lib": [
      "ESNext"
    ],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "removeComments": true,
    "sourceMap": true
  }
}